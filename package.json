{
  "name": "donaldboulton",
  "version": "4.10.0",
  "description": "Minimal Mistakes Jekyll theme npm build scripts",
  "repository": {
    "type": "git",
    "url": "git://github.com/donaldboulton/DWB.git"
  },
  "keywords": [
    "jekyll",
    "theme",
    "minimal"
  ],
  "author": "Donald Boulton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/donaldboulton/DWB/issues"
  },
  "homepage": "https://donaldboulton.github.io/DWB/",
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "npm-run-all": "^1.8.0",
    "onchange": "^2.2.0",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "uglify": "uglifyjs assets/js/vendor/jquery/jquery-3.3.1.min.js assets/js/plugins/jquery.fitvids.js assets/js/plugins/jquery.greedy-navigation.js assets/js/plugins/jquery.magnific-popup.js assets/js/plugins/jquery.smooth-scroll.min.js assets/js/_main.js -c -m -o assets/js/main.min.js",
    "add-banner": "node banner.js",
    "watch": "npm run watch:js & npm run watch:css",
    "watch:js": "onchange \"assets/js/**/*.js\" -e \"assets/js/main.min.js\" -- npm run build:js",
    "build:js": "npm run uglify && npm run add-banner",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --watch scss -o css"
  },
  "dependencies": {
    "async": "^2.6.0",
    "build-css": "^0.2.0",
    "less": "^3.0.1",
    "mocha": "^5.0.1",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "should": "^13.2.1"
  }
}
